{
    "compilerOptions": {
        "target": "ES2015",
        "lib": ["DOM", "DOM.Iterable","ES2015"],
        "allowJs": true,
        "skipLibCheck": true,
        "esModuleInterop": true,
        "strict": true,
        "forceConsistentCasingInFileNames": true,
        "noFallthroughCasesInSwitch": true,
        "module": "ESNext",
        "moduleResolution": "Node",
        "jsx": "react-jsx"
    },
    "include": ["src"]
}